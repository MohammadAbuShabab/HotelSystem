@model IEnumerable<GuestDetailsOutputModel>

@{ ViewData["Title"] = "Guests"; }

<div class="row">
    <div class="col-lg-4">
        <h2>All Dealers</h2>
    </div>
</div>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">EGN</th>
            <th scope="col">Card number</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var guest in Model)
        {
<tr>
    <td>@guest.Id</td>
    <td>@guest.Name</td>
    <td>@guest.EGN</td>
    <td>@guest.CardNumber</td>
    <td>
        <a class="btn btn-primary"
           asp-area=""
           asp-controller="Guests"
           asp-action="Edit"
           asp-route-id="@guest.Id">
            Edit
        </a>
    </td>
</tr>
}
    </tbody>
</table>
